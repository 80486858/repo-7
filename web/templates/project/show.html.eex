<h2>Show project</h2>

<ul>

  <li>
    <strong>Repo:</strong>
    <%= @project.repo_id %>
  </li>

  <li>
    <strong>Owner:</strong>
    <%= @project.owner %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @project.name %>
  </li>

</ul>

<%= link "Edit", to: project_path(@conn, :edit, @project) %>
<%= link "Back", to: project_path(@conn, :index) %>
