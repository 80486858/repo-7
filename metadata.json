{
    "name": "tohuwabohu-openntp",
    "version": "2.0.0",
    "author": "Martin Meinhold",
    "license": "Apache-2.0",
    "summary": "Puppet module for OpenNTPD",
    "source": "https://github.com/tohuwabohu/puppet-openntp.git",
    "project_page": "https://github.com/tohuwabohu/puppet-openntp",
    "issues_url": "https://github.com/tohuwabohu/puppet-openntp/issues",
    "types": [ "openntp", "ntp", "time" ],
    "operatingsystem_support": [
        {
            "operatingsystem": "Debian",
            "operatingsystemrelease": [
                "6",
                "7"
            ]
        },
        {
            "operatingsystem": "Ubuntu",
            "operatingsystemrelease": [
                "12.04",
                "14.04"
            ]
        }
    ],
    "requirements": [
      {
        "name": "puppet",
        "version_requirement": "3.x"
      }
    ],
    "dependencies": [
        {
          "name": "ripienaar/module_data",
          "version_requirement": "0.x"
        },
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": "4.x"
        }
    ]
}
